    /* Make the √ó button match the alert text */
    .alert .btn-close {
        position: absolute;
        top: 45%;
        right: 0.4rem;
        transform: translateY(-50%);
        background: none;
        opacity: 1;
        font-size: 1.8rem;
        font-weight: bold;
        line-height: 1;
        text-shadow: none;
        border: none;
        color: inherit; /* match the alert text color */
