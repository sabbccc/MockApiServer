            // üöÄ AJAX form submission for create/edit
            $(document).on('submit', 'form#createApplicationForm, form#editApplicationForm', function (e) {
                e.preventDefault();
                const form = $(this);
                $.ajax({
                    url: form.attr('action'),
                    type: form.attr('method'),
